{
  "expo": {
    "name": "ClinReport",
    "slug": "clinreport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clinreport.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
