    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.clinreport.app"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/faeb4e5e-a74d-4b75-bc8b-de6522ffe558"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "description": "Clinical documentation intelligence powered by AI",
    "extra": {
      "eas": {
        "projectId": "faeb4e5e-a74d-4b75-bc8b-de6522ffe558"
      }
    }
  }
}
